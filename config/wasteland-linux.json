{
    "hostid": "wasteland-linux",
    "hostname": "wasteland",
    "browsers": ["iceweasel", "firefox", "chrome", "chromium", "midori"],
    "os": {
        "type": "linux",
        "flavor": "ubuntu"
    },
    "shell": {
        "ohmyzsh_theme": "gnzh",
        "iterm_tmux_integration": true,
        "use_default_llvm": true
    },
    "path": {
        "pin": "$HOME/software/pin",
        "cliutils": "$HOME/git/cliutils",
        "ohmyzsh": "$HOME/.oh-my-zsh",
        "llvm": "$HOME/software/llvm-cpi"
    }
}
