{
    "browsers": ["iceweasel", "firefox", "chrome", "chromium", "midori"],
    "os": {
        "type": "linux",
        "flavor": "ubuntu"
    },
    "shell": {
        "use_default_llvm": true
    },
    "path": {
        "ffmpeg": "$HOME/software/ffmpeg",
        "gcloud_sdk": "$HOME/software/google-cloud-sdk",
        "java": "/usr/lib/jvm/default-java",
        "ocaml_opam": "$HOME/software/ocaml",
        "rust_cargo": "$HOME/software/cargo",
        "rust_rustup": "$HOME/software/rustup"
    }
}
